  "scripts": {
    "pre-commit": "eslint",
    "pre-push": "eslint",
    "start": "env-cmd -f .env.development --use-shell 'react-app-rewired start'",
    "start:staging": "env-cmd -f .env.staging --use-shell 'react-app-rewired start'",
    "start:prod": "env-cmd -f .env.production --use-shell 'react-app-rewired start'",
    "build:dev": "env-cmd -f .env.development --use-shell 'react-app-rewired build'",
    "build:staging": "env-cmd -f .env.staging --use-shell 'react-app-rewired build'",
    "build:prod": "env-cmd -f .env.production --use-shell 'react-app-rewired build'",
    "upload-sourcemaps:dev": "env-cmd -f .env.development node src/utils/bugsnag/process-sourcemaps.js",
    "test": "react-app-rewired test",
    "extract-translation-keys": "cd src && cd utils && cd i18next && node generateLocaleMaster & cd ../../../"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
